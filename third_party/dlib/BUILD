        "export TMP_DIR=$$(mktemp -d -t dlib.XXXXXX)",
        "mkdir -p $$TMP_DIR",
        "cp -R $$(pwd)/external/dlib_archive/* $$TMP_DIR",
        "cd $$TMP_DIR",
        "mkdir build",
        "cd build",
        "cmake -D CMAKE_INSTALL_PREFIX=$$INSTALL_DIR -DCMAKE_C_COMPILER=$(CC) -DCMAKE_CXX_COMPILER=$(CC) ..",
        "echo $$INSTALL_DIR",
        "make install",
        "ls $$INSTALL_DIR",
        "rm -rf $$TMP_DIR",
    ]),
    toolchains = [
        "@bazel_tools//tools/cpp:current_cc_toolchain",
    ],
)

cc_library(
    name = "dlib",
    srcs = lib_files,
    hdrs = include_files,
    includes = ["dlib/include"],
    linkstatic = 1,
)

filegroup(
    name = "dlib-files",
    srcs = include_files + lib_files,
)
